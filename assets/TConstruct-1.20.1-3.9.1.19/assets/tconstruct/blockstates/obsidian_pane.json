{
  "multipart": [
    {
      "apply": {
        "model": "tconstruct:block/obsidian_pane/post"
      }
    },
    {
      "apply": {
        "model": "tconstruct:block/obsidian_pane/side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "tconstruct:block/obsidian_pane/noside"
      },
      "when": {
        "north": "false"
      }
    },
    {
      "apply": {
        "model": "tconstruct:block/obsidian_pane/side_alt"
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "tconstruct:block/obsidian_pane/noside_alt",
        "y": 90
      },
      "when": {
        "south": "false"
      }
    },
    {
      "apply": {
        "model": "tconstruct:block/obsidian_pane/side_alt",
        "y": 90
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "tconstruct:block/obsidian_pane/noside",
        "y": 270
      },
      "when": {
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "tconstruct:block/obsidian_pane/side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "tconstruct:block/obsidian_pane/noside_alt"
      },
      "when": {
        "east": "false"
      }
    }
  ]
}